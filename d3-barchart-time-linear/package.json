{
  "name": "vanilla-typescript",
  "version": "1.0.0",
  "description": "JavaScript and TypeScript example starter project",
  "main": "index.html",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist",
    "copy-data": "cp -r ./data.csv dist",
    "init": "yarn clean-dist && yarn copy-data",
    "start": "npm run init && parcel index.html --open",
    "build": "parcel build index.html"
  },
  "dependencies": {
    "d3": "^7.4.4",
    "parcel-bundler": "^1.6.1"
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "regenerator-runtime": "^0.13.9",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "typescript",
    "javascript"
  ]
}
